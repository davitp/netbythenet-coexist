{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },

  "AllowedHosts": "*",

  "spring" : {
    "application" : {
      "name" : "identity"
    }
  },

  "eureka" : {
    
    "client" : {
      "shouldRegisterWithEureka" : true,
      "serviceUrl" : "http://discovery:33001/eureka",
      "ValidateCertificates":  false
    },

    "instance" : {
      "port" : "33002",
      "preferIpAddress": true
    }
  }
}
